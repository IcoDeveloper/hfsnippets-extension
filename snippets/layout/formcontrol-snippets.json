    "TextFieldTel": {
        "scope": "html",
        "prefix": "hf-textfield-tel",
        "body": [
            "<input id=\"${1:id}\"",
            "    type=\"tel\"",
            "    data-win-control=\"HFWinJSCtrl.TextField\"",
            "    data-win-options=\"{",
            "        label: '$2',",
            "        tooltip: '',",
            "        required: ${3:false},",
            "        disabled: ${4:false},",
            "        validator: {",
            "            pattern: '[\\+]{1}[0-9]{1,3}[ ]{1}[\\(]{0,1}[1-9][0-9]{0,3}[\\)]{0,1}[ ]{1}[0-9 ]{3,15}',",
            "            errorText: '$5',",
            "        },",
            "    }\">"
        ],
        "description": "Creates a Text input field"
    },
    "TextArea": {
        "scope": "html",
        "prefix": "hf-textarea",
        "body": [
            "<textarea id=\"${1:id}\"",
            "    rows=\"1\"",
            "    data-win-control=\"HFWinJSCtrl.TextField\"",
            "    style=\"min-height:${2:110px}; height:auto\"",
            "    data-win-options=\"{",
            "        label: '$3',",
            "        tooltip: '',",
            "        required: ${4:false},",
            "        disabled: ${5:false},",
            "        autoResize: ${6:true},",
            "        charCounter: ${7:false},",
            "    }\"></textarea>"
        ],
        "description": "Creates a textarea"
    },
