{
    "ButtonControl": {
        "scope": "html",
        "prefix": "hf-buttoncontrol",
        "body": [
            "<div id=\"${1:id}\"",
            "    data-win-control=\"HFWinJSCtrl.ButtonControl\"",
            "    data-win-options=\"{",
            "        buttonType: '${2:large}',",
            "        buttonHeading: '${3:heading}',",
            "        buttonLabel: '${4:label}',",
            "        callback: ${5:callback},",
            "    }\"></div>"
        ],
        "description": "Creates a ButtonControl"
    },
    "CheckBox": {
        "scope": "html",
        "prefix": "hf-checkbox",
        "body": [
            "<input id=\"${1:id}\"",
            "    type=\"checkbox\"",
            "    value=\"${2:value}\"",
            "    data-win-control=\"HFWinJSCtrl.CheckBox\"",
            "    data-win-options=\"{",
            "        label: '$3',",
            "        defaultValue: ${4:false},",
            "        style: '${5:backgroundHighlight}',",
            "    }\">"
        ],
        "description": "Creates a Checkbox"
    },
