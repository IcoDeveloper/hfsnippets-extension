    "RepeatingInput": {
        "prefix": "hf-repeatinginput",
        "body": [
            "<div class=\"repeating-input-container\">",
            "    <div class=\"grid column1\">",
            "        <div class=\"r1 c1\">",
            "            <div id=\"${1:id}\"",
            "                type=\"text\"",
            "                data-win-control=\"HFWinJSCtrl.Label\"",
            "                data-win-options=\"{ ",
            "                    label: '${2:label}',",
            "                    required: true,",
            "                    requiredFields: '${1:id}_input_1'",
            "                }\"></div>",
            "                   ",
            "        </div>",
            "    </div>",
            "    <div class=\"grid column1 repeating-input init\">",
            "        <div class=\"r1 c1\">",
            "            <input id=\"${1:id}_input_1\"",
            "                type=\"text\"",
            "                data-win-control=\"HFWinJSCtrl.TextField\">",
            "            <div id=\"${1:id}_remove_input_1\"",
            "                class=\"remove-input pdf-hide\"",
            "                data-win-control=\"HFFormdefinition.${3:Namespace}.removeInput\">",
            "                <p class=\"remove-icon\"><i class=\"fal fa-minus\"></i></p>",
            "            </div>",
            "        </div>",
            "    </div>",
            "    <div class=\"grid column1 repeating-input\">",
