	"TextFieldTel": {
		"scope": "html",
		"prefix": "hf-textfield-tel",
		"body": [
			"<input id=\"${1:id}\"",
            "    type=\"tel\"",
            "    data-win-control=\"HFWinJSCtrl.TextField\"", 
			"    data-win-options=\"{",
            "        label: '$2',",
            "        validator: {",
            "            pattern: '[\\+]{1}[0-9]{1,3}[ ]{1}[\\(]{0,1}[1-9][0-9]{0,3}[\\)]{0,1}[ ]{1}[0-9 ]{3,15}',",
            "            errorText: '$3'",
            "        }",
            "    }\">"
		],
		"description": "Creates a Text input field"
	},

