{
	"Condition": {
		"scope": "html",
		"prefix": "hf-condition",
		"body": [
			"<div id=\"${1:id}\" data-win-control=\"HFWinJSCtrl.Condition\"",
			"\tdata-hf-condition='{\"cond\": [",
                "\t\t{ \"type\": \"$2\", \"id\": \"$3\", \"val\": \"$4\" }",
			"\t], \"else\": \"$5\"}'>",
			"\t$6",
			"</div>"
		],
		"description": "Creates a block, area, tab, condition definition"
	},

	"Page Condition": {
		"scope": "html",
		"prefix": "hf-pagecondition",
		"body": [
			"<li data-hf-title=\"$1\"", 
			"\tdata-hf-condition='{\"cond\": [",
				"\t\t{ \"type\": \"$2\", \"id\": \"$3\", \"val\": \"$4\" }",
			"\t], \"else\": \"$5\"}'>",
			"\t<ol>",
				"\t\t$6",
			"\t</ol>",
			"</li>"
		],
		"description": "Creates a Page condition definition"
	},

	"Template Condition": {
		"scope": "html",
		"prefix": "hf-templatecondition",
		"body": [
			"<li data-hf-title=\"${1:Tab Name}\"", 
			"\tdata-hf-condition='{\"cond\": [",
				"\t\t { \"type\": \"$2\", \"id\": \"$3\", \"val\": \"$4\" }",
				"\t], \"else\": \"$5\"}'>",
				"\t$6",
            "</li>"
		],
		"description": "Creates a Template condition definition"
	},

	"Template": {
		"scope": "html",
		"prefix": "hf-block",
		"body": [
			"<div id=\"${1:id}\" data-win-control=\"WinJS.Binding.Template\">", 
				"\t$2",
			"</div>"
		],
		"description": "Creates an empty block container"
	},

    "ButtonControl": {
        "scope": "html",
        "prefix": "hf-buttoncontrol",
        "body": [
