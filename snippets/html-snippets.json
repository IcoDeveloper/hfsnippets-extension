    "RepeatingInput": {
        "prefix": "hf-repeatinginput",
        "body": [
            "<div class=\"repeating-input-container\">",
            "    <div class=\"grid column1\">",
            "        <div class=\"r1 c1\">",
            "            <div id=\"${1:id}\"",
            "                type=\"text\"",
            "                data-win-control=\"HFWinJSCtrl.Label\"",
            "                data-win-options=\"{ ",
            "                    label:'${2:label}',",
            "                    required: true,",
            "                    requiredFields: '${1:id}_input_1'",
            "                }\" ></div>",
            "                   ",
            "        </div>",
            "    </div>",
            "    <div class=\"grid column1 repeating-input init\">",
            "        <div class=\"r1 c1\">",
            "            <input id=\"${1:id}_input_1\" type=\"text\" data-win-control=\"HFWinJSCtrl.TextField\" />",
            "            <div id=\"${1:id}_remove_input_1\" class=\"remove-input pdf-hide\" data-win-control=\"HFFormdefinition.${3:Namespace}.removeInput\">",
            "                <p class=\"remove-icon\"><i class=\"fal fa-minus\"></i></p>",
            "            </div>",
            "        </div>",
            "    </div>",
            "    <div class=\"grid column1 repeating-input\">",
            "        <div class=\"r1 c1\">",
            "            <input id=\"${1:id}_input_2\" type=\"text\" data-win-control=\"HFWinJSCtrl.TextField\" />",
            "            <div id=\"${1:id}_remove_input_2\" class=\"remove-input pdf-hide\" data-win-control=\"HFFormdefinition.${3:Namespace}.removeInput\">",
            "                <p class=\"remove-icon\"><i class=\"fal fa-minus\"></i></p>",
            "            </div>",
            "        </div>",
            "    </div>",
            "    <div class=\"grid column1 repeating-input\">",
            "        <div class=\"r1 c1\">",
            "            <input id=\"${1:id}_input_3\" type=\"text\" data-win-control=\"HFWinJSCtrl.TextField\" />",
            "            <div id=\"${1:id}_remove_input_3\" class=\"remove-input pdf-hide\" data-win-control=\"HFFormdefinition.${3:Namespace}.removeInput\">",
            "                <p class=\"remove-icon\"><i class=\"fal fa-minus\"></i></p>",
            "            </div>",
            "        </div>",
            "    </div>",
            "    <div class=\"grid column1 repeating-input\">",
            "        <div class=\"r1 c1\">",
            "            <input id=\"${1:id}_input_4\" type=\"text\" data-win-control=\"HFWinJSCtrl.TextField\" />",
            "            <div id=\"${1:id}_remove_input_4\" class=\"remove-input pdf-hide\" data-win-control=\"HFFormdefinition.${3:Namespace}.removeInput\">",
            "                <p class=\"remove-icon\"><i class=\"fal fa-minus\"></i></p>",
            "            </div>",
            "        </div>",
            "    </div>",
            "    <div class=\"grid column1 repeating-input\">",
            "        <div class=\"r1 c1\">",
            "            <input id=\"${1:id}_input_5\" type=\"text\" data-win-control=\"HFWinJSCtrl.TextField\" />",
            "            <div id=\"${1:id}_remove_input_5\" class=\"remove-input pdf-hide\" data-win-control=\"HFFormdefinition.${3:Namespace}.removeInput\">",
            "                <p class=\"remove-icon\"><i class=\"fal fa-minus\"></i></p>",
            "            </div>",
            "        </div>",
            "    </div>",
            "    <div class=\"grid column1 repeating-input\">",
            "        <div class=\"r1 c1\">",
            "            <input id=\"${1:id}_input_6\" type=\"text\" data-win-control=\"HFWinJSCtrl.TextField\" />",
            "            <div id=\"${1:id}_remove_input_6\" class=\"remove-input pdf-hide\" data-win-control=\"HFFormdefinition.${3:Namespace}.removeInput\">",
            "                <p class=\"remove-icon\"><i class=\"fal fa-minus\"></i></p>",
            "            </div>",
            "        </div>",
            "    </div>",
            "    <div class=\"grid column1 repeating-input\">",
            "        <div class=\"r1 c1\">",
            "            <input id=\"${1:id}_input_7\" type=\"text\" data-win-control=\"HFWinJSCtrl.TextField\" />",
            "            <div id=\"${1:id}_remove_input_7\" class=\"remove-input pdf-hide\" data-win-control=\"HFFormdefinition.${3:Namespace}.removeInput\">",
            "                <p class=\"remove-icon\"><i class=\"fal fa-minus\"></i></p>",
            "            </div>",
            "        </div>",
            "    </div>",
            "    <div class=\"grid column1 repeating-input\">",
            "        <div class=\"r1 c1\">",
            "            <input id=\"${1:id}_input_8\" type=\"text\" data-win-control=\"HFWinJSCtrl.TextField\" />",
            "            <div id=\"${1:id}_remove_input_8\" class=\"remove-input pdf-hide\" data-win-control=\"HFFormdefinition.${3:Namespace}.removeInput\">",
            "                <p class=\"remove-icon\"><i class=\"fal fa-minus\"></i></p>",
            "            </div>",
            "        </div>",
            "    </div>",
            "    <div class=\"grid column1 repeating-input\">",
            "        <div class=\"r1 c1\">",
            "            <input id=\"${1:id}_input_9\" type=\"text\" data-win-control=\"HFWinJSCtrl.TextField\" />",
            "            <div id=\"${1:id}_remove_input_9\" class=\"remove-input pdf-hide\" data-win-control=\"HFFormdefinition.${3:Namespace}.removeInput\">",
            "                <p class=\"remove-icon\"><i class=\"fal fa-minus\"></i></p>",
            "            </div>",
            "        </div>",
            "    </div>",
            "    <div class=\"grid column1 repeating-input\">",
            "        <div class=\"r1 c1\">",
            "            <input id=\"${1:id}_input_10\" type=\"text\" data-win-control=\"HFWinJSCtrl.TextField\" />",
            "            <div id=\"${1:id}_remove_input_10\" class=\"remove-input pdf-hide\" data-win-control=\"HFFormdefinition.${3:Namespace}.removeInput\">",
            "                <p class=\"remove-icon\"><i class=\"fal fa-minus\"></i></p>",
            "            </div>",
            "        </div>",
            "    </div>",
            "    <div class=\"grid column3 add-input-wrapper\">",
            "        <div class=\"r1 c1\">&nbsp;</div>",
            "        <div class=\"r1 c2\">&nbsp;</div>",
            "        <div class=\"r1 c3\">",
            "            <div id=\"${1:id}_add_input\" class=\"add-input pdf-hide\" data-win-control=\"HFFormdefinition.${3:Namespace}.addInput\"",
            "                data-win-options=\"{",
            "                    inputId: '${1:id}_'",
            "                }\">",
            "                <p><strong>Hinzuf√ºgen</strong></p>",
            "            </div>",
            "        </div>",
            "    </div>",
            "</div>"
        ],
        "description": "Add html structure for RepeatingInput"
    }
