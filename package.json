{
	"name": "hfsnippets",
	"displayName": "HybridForms Snippets",
	"description": "Integrates HybridForms HTML snippets into VS Code",
	"version": "1.1.3",
	"publisher": "icomedias",
	"icon": "HF_Extension_icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/IcoDeveloper/hfsnippets-extension.git"
	},
	"engines": {
		"vscode": "^1.28.0"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
		"snippets": [
			{
				"language": "html",
				"path": "./html-snippets.json"
			},
			{
				"language": "javascript",
				"path": "./javascript-snippets.json"
			}
		]
	}
}
